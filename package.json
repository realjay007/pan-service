		"errorhandler": "^1.5.0",
		"express": "^4.16.4",
		"express-bearer-token": "^2.2.0",
		"minified-headless": "^0.1.0",
		"moment": "^2.22.2",
		"mongodb": "^3.1.8",
		"mongodb-uri": "^0.9.7",
		"mongoose": "^5.7.5",
		"nodemailer": "^4.6.8",
		"pug": "^2.0.3",
		"randomstring": "^1.1.5",
		"request": "^2.88.0",
		"request-promise": "^4.2.2",
		"ts-node": "^7.0.1",
